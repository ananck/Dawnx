{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "experimentalDecorators": true,
    "noImplicitUseStrict": true
  },
  "compileOnSave": true,
  "include": [ "**/*" ],
  "exclude": [
    "obj",
    "bin",
    "node_modules"
  ]
}
