<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup Label="Globals">
    <SccProjectName>SAK</SccProjectName>
    <SccProvider>SAK</SccProvider>
    <SccAuxPath>SAK</SccAuxPath>
    <SccLocalPath>SAK</SccLocalPath>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.5.36</Version>
    <Authors>Dawnx Contributors</Authors>
    <Owners>zmjack</Owners>
    <Company>dawnx.net</Company>
    <Description>Provides IdentityUtility for AspNetCoreApp 2.1+. Use it to manage IdentityUsers and IdentityRoles easily.</Description>
    <PackageLicenseUrl></PackageLicenseUrl>
    <Copyright>Copyright © dawnx.net 2018</Copyright>
    <PackageIconUrl>http://dawnx.net/logo32.png</PackageIconUrl>
    <PackageProjectUrl>https://github.com/zmjack/Dawnx</PackageProjectUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DebugType>none</DebugType>
    <DebugSymbols>false</DebugSymbols>
    <DocumentationFile>D:\TeamProjects\Dawnx\~Library\~AspNetCore\Dawnx.AspNetCore.IdentityUtility\Dawnx.AspNetCore.IdentityUtility.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
